{
  "name": "munu",
  "private": true,
  "author": {
    "name": "Falqon",
    "url": "https://falqon.com.br"
  },
  "scripts": {
    "apps:build": "yarn workspaces foreach -ptv --include '*-app' run build",
    "apps:clean": "yarn workspaces foreach -ptv --include '*-app' run clean",
    "clean:global-cache": "rimraf ./.cache",
    "g:build": "yarn workspaces foreach -p --topological-dev -v run build",
    "g:build-changed": "yarn workspaces foreach --topological-dev --no-private -v --since=origin/main run build",
    "g:changeset": "changeset",
    "g:check-dist": "yarn workspaces foreach -ptv run check-dist",
    "g:clean": "yarn clean:global-cache && yarn workspaces foreach -ptv run clean",
    "g:fix-all-files": "yarn workspaces foreach -ptv run fix-all-files",
    "g:lint": "yarn workspaces foreach -ptv run lint --color",
    "g:lint-staged-files": "lint-staged --allow-empty",
    "g:lint-styles": "yarn workspaces foreach -ptv run lint-styles --color",
    "g:release": "yarn g:build && changeset publish",
    "g:share-static-hardlink": "yarn workspaces foreach -pv --include '*-app' run share-static-hardlink",
    "g:share-static-symlink": "yarn workspaces foreach -pv --include '*-app' run share-static-symlink",
    "g:test-e2e": "yarn workspaces foreach -tv run test-e2e",
    "g:test-unit": "yarn workspaces foreach -ptv run test-unit",
    "g:test-unit-jest": "yarn workspaces foreach -ptv run test-unit-jest",
    "g:typecheck": "yarn workspaces foreach -ptv run typecheck",
    "nuke:node_modules": "npx rimraf@3 '**/node_modules'",
    "packages:build": "yarn workspaces foreach -ptv --include '@munu/*' run build",
    "packages:clean": "yarn workspaces foreach -ptv --include '@munu/*' run clean",
    "packages:lint": "yarn workspaces foreach -ptv --include '@munu/*' run lint",
    "packages:test:unit": "yarn workspaces foreach -ptv --include '@munu/*' run test-unit",
    "packages:typecheck": "yarn workspaces foreach -ptv --include '@munu/*' run typecheck",
    "s:app": "yarn workspace munu-app dev",
    "production:app": "yarn workspace munu-app build --mode production",
    "production:all": "npm-run-all production:app production:web && node prepareBuild.js",
    "staging:app": "yarn workspace munu-app build --mode staging",
    "staging:all": "npm-run-all staging:app staging:web && node prepareBuild.js"
  },
  "dependencies": {
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.0",
    "@hookform/resolvers": "2.9.11",
    "@mui/material": "5.15.14",
    "@mui/styles": "5.11.16",
    "@sentry/react": "7.99.0",
    "@sentry/vite-plugin": "2.13.0",
    "@tanstack/react-query": "4.24.10",
    "@typeform/embed-react": "3.15.0",
    "@types/payment": "2.1.4",
    "@types/react-copy-to-clipboard": "5.0.4",
    "@types/react-slick": "0.23.10",
    "@web3-onboard/core": "2.21.3",
    "@web3-onboard/injected-wallets": "2.10.13",
    "cronstrue": "2.26.0",
    "cross-env": "7.0.3",
    "date-fns-tz": "2.0.0",
    "ethers": "6.11.1",
    "gerador-validador-cpf": "5.0.2",
    "js-file-download": "0.4.12",
    "react-arborist": "3.2.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-hook-form": "7.43.2",
    "react-qr-code": "2.0.11",
    "react-slick": "0.29.0",
    "slick-carousel": "1.8.1",
    "use-resize-observer": "9.1.0",
    "victory": "36.6.11",
    "zustand": "4.3.5"
  },
  "devDependencies": {
    "@munu/eslint-config-bases": "workspace:^",
    "@types/prettier": "2.7.2",
    "@types/shell-quote": "1.7.1",
    "cp-cli": "2.0.0",
    "eslint": "8.35.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "lint-staged": "13.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "shell-quote": "1.8.0",
    "syncpack": "9.8.4",
    "typescript": "4.9.5",
    "typescript-plugin-css-modules": "5.0.1"
  },
  "engines": {
    "node": "^14.18.3 || >=16.13.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "packageManager": "yarn@4.0.0-rc.39",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
